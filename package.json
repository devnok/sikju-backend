  "author": "devnok <ha819ha.dev@gmail.com>",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/runtime": "^7.9.6",
    "@prisma/client": "^2.0.0-beta.2",
    "aws-sdk": "^2.637.0",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.7.6",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-fetch": "^2.6.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "prisma-client-lib": "^1.34.10",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@prisma/cli": "^2.0.0-beta.5",
    "nodemon": "^2.0.4"
  },
  "scripts": {
    "studio": "prisma studio --port 7777 --experimental",
    "migrate": "prisma migrate save --experimental && prisma migrate up --experimental",
    "generate": "prisma generate",
    "prisma": "yarn run migrate && yarn run generate",
    "dev": "nodemon --exec babel-node src/server.js",
    "build": "babel src -d build",
    "postbuild": "cd src && npx copy ./api/**/*.graphql ../build/api",
    "start": "node build/server.js"
  }
}
